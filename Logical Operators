x = True
y = False

print(x and y) #true and false
print(x or y) # true of true
print(not x) # not true = false
print(not y) # not false = true

x = 5
print(x > 4 and x < 11) # True
print(x > 3 or x < 4) # True
print(not(x > 3 and x < 10)) # false

x = 100
print(x > 40 and x < 90) # false 
print(x > 30 or x < 400) # true
print(not(x > 3 and x < 10)) # true

